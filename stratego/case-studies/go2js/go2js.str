    PrimaryExprArguments     : PrimaryExpr * Arguments -> PrimaryExpr
    PrimaryExprIndex         : PrimaryExpr * Index -> PrimaryExpr
    PrimaryExprSlice         : PrimaryExpr * Slice -> PrimaryExpr
