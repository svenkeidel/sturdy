    LiteralValue             : Option(ElementList) -> LiteralValue
    ElementList              : List(KeyedElement) -> ElementList
    SingleElem               : Element -> KeyedElement
    KeyValPair               : Key * Element -> KeyedElement
                             : Identifier -> Key
