                             : SimpleStmt -> InitStmt
                             : SimpleStmt -> PostStmt
    RangeClause              : Option(RangeExpIdList) * Expression -> RangeClause
    ExpressionList           : ExpressionList -> RangeExpIdList
    IdentifierList           : IdentifierList -> RangeExpIdList
