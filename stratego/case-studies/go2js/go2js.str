    TypeSpec                 : Identifier * Type -> TypeSpec
    VarDecl                  : VarSpec -> VarDecl
    VarSpecTyped             : IdentifierList * Type * Option(VarDeclAssign) -> VarSpec
    VarSpec                  : IdentifierList * VarDeclAssign -> VarSpec
